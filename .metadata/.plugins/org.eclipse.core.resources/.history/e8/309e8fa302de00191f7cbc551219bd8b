		myIntQueue = null;
		myStringQueue = null;
	}

	@Test
	public void testTwoEmptyQueues() {
		FifoQueue<Integer> q1 = new FifoQueue<Integer>();
		FifoQueue<Integer> q2 = new FifoQueue<Integer>();
		q1.append(q2);
		
	}
	
