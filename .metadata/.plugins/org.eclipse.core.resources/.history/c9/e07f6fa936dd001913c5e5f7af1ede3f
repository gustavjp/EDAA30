package textproc;
import java.util.*;

public class SetGetter {
	Scanner s;
	Set<String> theSet;
	
	public SetGetter(Scanner s) {
		this.s = s;
		theSet = new HashSet<String>();
	}
	
	public Set<String> getSet() {
		Set<String> theSet = new HashSet<String>();
		while(s.hasNext()) {
			String word = s.next().toLowerCase();
			theSet.add(word);
		}
		return theSet;
	}
}